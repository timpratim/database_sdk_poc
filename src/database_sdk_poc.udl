namespace database_sdk_poc {
    
    struct RecordId {
        tb: string;
        id: string;
    }

    struct Person {
        id: RecordId;
        firstname: string;
        lastname: string;
        age?: u32;
    }

    interface DatabaseSDKPOC {
        create(id: RecordId, firstname: string, lastname: string) -> Person;
    }
}
